<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="628.0" prefWidth="802.0" style="-fx-background-color: #95CB72;"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.controller.StudentApplicationController">
    <children>
        <Label alignment="CENTER" layoutX="39.0" layoutY="14.0" prefHeight="52.0" prefWidth="185.0"
               style="-fx-border-color: #000;" text="All Applications">
            <font>
                <Font name="Gloucester MT Extra Condensed" size="33.0"/>
            </font>
        </Label>
        <TableView fx:id="applications_table" layoutX="38.0" layoutY="152.0" prefHeight="396.0" prefWidth="725.0"
                   style="-fx-background-color: #C9DFD5;">
            <columns>
                <TableColumn fx:id="job_id" prefWidth="91.19995498657227" text="Job ID"/>
                <TableColumn fx:id="comp_name" prefWidth="161.6000213623047" text="Company name"/>
                <TableColumn fx:id="desc" minWidth="0.0" prefWidth="381.5999755859375" text="Description"/>
                <TableColumn fx:id="status" minWidth="0.0" prefWidth="91.199951171875" text="Status"/>
            </columns>
        </TableView>
        <Button layoutX="373.0" layoutY="576.0" mnemonicParsing="false" onAction="#Back" text="Back" textFill="RED">
            <font>
                <Font size="16.0"/>
            </font>
        </Button>
        <Label fx:id="name" layoutX="39.0" layoutY="91.0" prefHeight="27.0" prefWidth="116.0" text="Name"
               textFill="RED">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <Button fx:id="bt_home_page" layoutX="648.0" layoutY="24.0" mnemonicParsing="false" onAction="#BackToHomePage"
                prefHeight="27.0" prefWidth="116.0" text="Home page">
            <font>
                <Font size="15.0"/>
            </font>
        </Button>

    </children>
</AnchorPane>
